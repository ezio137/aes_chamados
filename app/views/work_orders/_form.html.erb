<%= form_for @work_order, :html => { :class => 'form-horizontal' } do |f| %>
  <div class="control-group">
    <%= f.label :service_call_id, :class => 'control-label' %>
    <div class="controls">
      <%= f.text_field :service_call_id, :class => 'text_field' %>
    </div>
  </div>
  <div class="control-group">
    <%= f.label :registration_created, :class => 'control-label' %>
    <div class="controls">
      <%= f.text_field :registration_created, :class => 'text_field' %>
    </div>
  </div>
  <div class="control-group">
    <%= f.label :description, :class => 'control-label' %>
    <div class="controls">
      <%= f.text_field :description, :class => 'text_field' %>
    </div>
  </div>
  <div class="control-group">
    <%= f.label :priority_id, :class => 'control-label' %>
    <div class="controls">
      <%= f.text_field :priority_id, :class => 'text_field' %>
    </div>
  </div>
  <div class="control-group">
    <%= f.label :status_id, :class => 'control-label' %>
    <div class="controls">
      <%= f.text_field :status_id, :class => 'text_field' %>
    </div>
  </div>
  <div class="control-group">
    <%= f.label :assignment_to_person_id, :class => 'control-label' %>
    <div class="controls">
      <%= f.text_field :assignment_to_person_id, :class => 'text_field' %>
    </div>
  </div>
  <div class="control-group">
    <%= f.label :customer_id, :class => 'control-label' %>
    <div class="controls">
      <%= f.text_field :customer_id, :class => 'text_field' %>
    </div>
  </div>
  <div class="control-group">
    <%= f.label :approval_result_id, :class => 'control-label' %>
    <div class="controls">
      <%= f.text_field :approval_result_id, :class => 'text_field' %>
    </div>
  </div>
  <div class="control-group">
    <%= f.label :information_log, :class => 'control-label' %>
    <div class="controls">
      <%= f.text_field :information_log, :class => 'text_field' %>
    </div>
  </div>
  <div class="control-group">
    <%= f.label :persons_modify_ticket_log, :class => 'control-label' %>
    <div class="controls">
      <%= f.text_field :persons_modify_ticket_log, :class => 'text_field' %>
    </div>
  </div>
  <div class="control-group">
    <%= f.label :last_person_to_modify_ticket, :class => 'control-label' %>
    <div class="controls">
      <%= f.text_field :last_person_to_modify_ticket, :class => 'text_field' %>
    </div>
  </div>
  <div class="control-group">
    <%= f.label :assignment_to_person_time, :class => 'control-label' %>
    <div class="controls">
      <%= f.text_field :assignment_to_person_time, :class => 'text_field' %>
    </div>
  </div>
  <div class="control-group">
    <%= f.label :assignment_to_workgroup_time, :class => 'control-label' %>
    <div class="controls">
      <%= f.text_field :assignment_to_workgroup_time, :class => 'text_field' %>
    </div>
  </div>
  <div class="control-group">
    <%= f.label :actual_finish, :class => 'control-label' %>
    <div class="controls">
      <%= f.text_field :actual_finish, :class => 'text_field' %>
    </div>
  </div>
  <div class="control-group">
    <%= f.label :approvers_required, :class => 'control-label' %>
    <div class="controls">
      <%= f.text_field :approvers_required, :class => 'text_field' %>
    </div>
  </div>
  <div class="control-group">
    <%= f.label :closure_code_id, :class => 'control-label' %>
    <div class="controls">
      <%= f.text_field :closure_code_id, :class => 'text_field' %>
    </div>
  </div>
  <div class="control-group">
    <%= f.label :deadline, :class => 'control-label' %>
    <div class="controls">
      <%= f.text_field :deadline, :class => 'text_field' %>
    </div>
  </div>
  <div class="control-group">
    <%= f.label :requestor_id, :class => 'control-label' %>
    <div class="controls">
      <%= f.text_field :requestor_id, :class => 'text_field' %>
    </div>
  </div>

  <div class="form-actions">
    <%= f.submit nil, :class => 'btn btn-primary' %>
    <%= link_to t('.cancel', :default => t("helpers.links.cancel")),
                work_orders_path, :class => 'btn' %>
  </div>
<% end %>
