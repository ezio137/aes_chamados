<%- model_class = WorkOrder -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.titleize %></h1>
</div>

<dl class="dl-horizontal">
  <dt><strong><%= model_class.human_attribute_name(:service_call_id) %>:</strong></dt>
  <dd><%= @work_order.service_call_id %></dd>
  <dt><strong><%= model_class.human_attribute_name(:registration_created) %>:</strong></dt>
  <dd><%= @work_order.registration_created %></dd>
  <dt><strong><%= model_class.human_attribute_name(:description) %>:</strong></dt>
  <dd><%= @work_order.description %></dd>
  <dt><strong><%= model_class.human_attribute_name(:priority_id) %>:</strong></dt>
  <dd><%= @work_order.priority_id %></dd>
  <dt><strong><%= model_class.human_attribute_name(:status_id) %>:</strong></dt>
  <dd><%= @work_order.status_id %></dd>
  <dt><strong><%= model_class.human_attribute_name(:assignment_to_person_id) %>:</strong></dt>
  <dd><%= @work_order.assignment_to_person_id %></dd>
  <dt><strong><%= model_class.human_attribute_name(:customer_id) %>:</strong></dt>
  <dd><%= @work_order.customer_id %></dd>
  <dt><strong><%= model_class.human_attribute_name(:approval_result_id) %>:</strong></dt>
  <dd><%= @work_order.approval_result_id %></dd>
  <dt><strong><%= model_class.human_attribute_name(:information_log) %>:</strong></dt>
  <dd><%= @work_order.information_log %></dd>
  <dt><strong><%= model_class.human_attribute_name(:persons_modify_ticket_log) %>:</strong></dt>
  <dd><%= @work_order.persons_modify_ticket_log %></dd>
  <dt><strong><%= model_class.human_attribute_name(:last_person_to_modify_ticket) %>:</strong></dt>
  <dd><%= @work_order.last_person_to_modify_ticket %></dd>
  <dt><strong><%= model_class.human_attribute_name(:assignment_to_person_time) %>:</strong></dt>
  <dd><%= @work_order.assignment_to_person_time %></dd>
  <dt><strong><%= model_class.human_attribute_name(:assignment_to_workgroup_time) %>:</strong></dt>
  <dd><%= @work_order.assignment_to_workgroup_time %></dd>
  <dt><strong><%= model_class.human_attribute_name(:actual_finish) %>:</strong></dt>
  <dd><%= @work_order.actual_finish %></dd>
  <dt><strong><%= model_class.human_attribute_name(:approvers_required) %>:</strong></dt>
  <dd><%= @work_order.approvers_required %></dd>
  <dt><strong><%= model_class.human_attribute_name(:closure_code_id) %>:</strong></dt>
  <dd><%= @work_order.closure_code_id %></dd>
  <dt><strong><%= model_class.human_attribute_name(:deadline) %>:</strong></dt>
  <dd><%= @work_order.deadline %></dd>
  <dt><strong><%= model_class.human_attribute_name(:requestor_id) %>:</strong></dt>
  <dd><%= @work_order.requestor_id %></dd>
</dl>

<div class="form-actions">
  <%= link_to t('.back', :default => t("helpers.links.back")),
              work_orders_path, :class => 'btn'  %>
  <%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_work_order_path(@work_order), :class => 'btn' %>
  <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              work_order_path(@work_order),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
</div>
